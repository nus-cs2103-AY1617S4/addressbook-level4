<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <GridPane fx:id="cardBackground" HBox.hgrow="ALWAYS">
      <columnConstraints>
         <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      </rowConstraints>
      <children>
         <HBox prefHeight="75.0" prefWidth="150.0">
            <children>
               <Label fx:id="id" minWidth="20.0" text="Label">
                  <padding>
                     <Insets left="5.0" />
                  </padding>
               </Label>
               <Label fx:id="name" text="Label" />
            </children>
         </HBox>
         <AnchorPane>
            <children>
               <Label fx:id="status" layoutX="21.0" layoutY="29.0" text="Label" />
               <FlowPane fx:id="tags" layoutX="21.0" layoutY="58.0" prefHeight="17.0" prefWidth="129.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="21.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="58.0" />
            </children>
         </AnchorPane>
      </children>
   </GridPane>
</HBox>
