<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <GridPane HBox.hgrow="ALWAYS">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <VBox alignment="CENTER_LEFT" minHeight="105" GridPane.columnIndex="0">
      <padding>
        <Insets top="5" right="5" bottom="5" left="15" />
      </padding>
      <HBox spacing="5" alignment="CENTER_LEFT">
        <Label fx:id="id" styleClass="cell_big_label">
          <minWidth>
            <!-- Ensures that the label text is never truncated -->
            <Region fx:constant="USE_PREF_SIZE" />
          </minWidth>
        </Label>
        <Label fx:id="name" text="\$first" styleClass="cell_big_label" />
        <AnchorPane >
        <Rectangle fx:id="ticksymbol" arcHeight="100.0" arcWidth="100.0" height="35.0"   width="35.0" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets top="50" right="50" bottom="50" left="50" />
         </HBox.margin>
      </Rectangle>
      </AnchorPane>
      </HBox>
      <FlowPane fx:id="tags" />
      <Label fx:id="time" styleClass="cell_small_label" text="\$time" />
      <Label fx:id="date" styleClass="cell_small_label" text="\$date" />
      <Label fx:id="taskType" styleClass="cell_small_label" text="\$taskType" />
      

    </VBox>
                
  </GridPane>
  

</HBox>
 